resource_name = "UI_Resources"
script/source = "# UI_Resources.gd (Affichage HUD)
extends Control

@onready var wood_label = $VBoxContainer/Wood
@onready var stone_label = $VBoxContainer/Stone
@onready var food_label = $VBoxContainer/Food

func _ready():
	ResourceManager.resource_changed.connect(_update_display)
	_update_display(\"\", 0)

func _update_display(_type: String, _amount: int):
	wood_label.text = \"Wood: %d\" % ResourceManager.get_resource(\"wood\")
	stone_label.text = \"Stone: %d\" % ResourceManager.get_resource(\"stone\")
	food_label.text = \"Food: %d\" % ResourceManager.get_resource(\"food\")
"

